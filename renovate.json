{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranches": ["develop"],
  "packageRules": [
    {
      "matchDatasources": ["regex", "repology"],
      "matchPackageNames": ["alpine/bash"],
      "versioning": "loose",
      "enabled": true
    }
  ]
}
